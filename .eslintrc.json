{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 13
  },
  "rules": {
    "no-prototype-builtins": "off",
    "operator-linebreak": ["error", "after"],
    "max-len": ["error", { "code": 80 }]
  },
  "plugins": ["jest"]
}
